{
  "name": "napp/aclcore",
  "description": "ACL core for projects",
  "keywords": [
    "napp",
    "acl",
    "roles",
    "permissions"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mads MÃ¸ller",
      "email": "mm@napp.dk"
    }
  ],
  "autoload": {
    "psr-4": {
      "Napp\\Core\\Acl\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Napp\\Core\\Acl\\Tests\\": "tests/"
    }
  },
  "require": {
    "php" : ">=7.1",
    "ext-json": "*",
    "illuminate/auth": "~5.6.0|~5.7.0",
    "illuminate/database": "~5.6.0|~5.7.0",
    "illuminate/support": "~5.6.0|~5.7.0",
    "illuminate/view": "~5.6.0|~5.7.0",
    "napp/apicore": "0.*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.7",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit" : "^7.0",
    "phpunit/phpcov": "^5.0",
    "squizlabs/php_codesniffer": "^3.1",
    "orchestra/testbench": "~3.6"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Napp\\Core\\Acl\\AclServiceProvider"
      ],
      "aliases": {}
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
