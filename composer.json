{
  "name": "napp/aclcore",
  "description": "ACL core for projects",
  "keywords": [
    "napp",
    "acl",
    "roles",
    "permissions"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mads MÃ¸ller",
      "email": "mm@napp.dk"
    }
  ],
  "autoload": {
    "psr-4": {
      "Napp\\Core\\Acl\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Napp\\Core\\Acl\\Tests\\": "tests/"
    }
  },
  "require": {
    "php" : "^7.2|^8.0",
    "ext-json": "*",
    "illuminate/auth": "^5.8|^6.0|^7.0|^8.0",
    "illuminate/database": "^5.8|^6.0|^7.0|^8.0",
    "illuminate/support": "^5.8|^6.0|^7.0|^8.0",
    "illuminate/view": "^5.8|^6.0|^7.0|^8.0",
    "napp/apicore": "^2.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpunit/phpcov": "^6.0|^7.0|^8.0",
    "squizlabs/php_codesniffer": "^3.5",
    "orchestra/testbench": "^5.0|^6.3"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Napp\\Core\\Acl\\AclServiceProvider"
      ],
      "aliases": {}
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
